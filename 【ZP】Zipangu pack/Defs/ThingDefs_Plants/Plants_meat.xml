<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="PlantBase" Class="Zipangu.ThingDef_Plant_Extend">
		<thingClass>Zipangu.Plant_Extend</thingClass>
		<MaxLeaflessTemperature>-60</MaxLeaflessTemperature>
		<MinLeaflessTemperature>-80</MinLeaflessTemperature>
		<defName>ZP_AquacultureFish</defName>
		<label>養殖魚</label>
		<description>生育期間は長いが非常に栄養価の高い魚を収穫することができる。</description>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/S_gyoei</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<nutrition>0.18</nutrition>
		</ingestible>
		<plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>true</reproduces>
			<maxMeshCount>4</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>ZP_Fish</harvestedThingDef>
			<harvestYield>10</harvestYield>
			<sowMinSkill>10</sowMinSkill>
			<sowTags>
				<li>ZP_FishTank</li>
			</sowTags>
			<topWindExposure>0.5</topWindExposure>
			<growDays>20</growDays>
			<visualSizeRange>
				<min>0.6</min>
				<max>0.8</max>
			</visualSizeRange>
			<sowResearchPrerequisites><li>ReAquacultureFish</li></sowResearchPrerequisites>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>ZP_PlantSoy</defName>
		<label>大豆</label>
		<description>畑のお肉である大豆を育成できる。成長は遅いが痩せた土地でも育つので重宝する。
        </description>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/daizuplant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<nutrition>0.18</nutrition>
		</ingestible>
		<plant>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<maxMeshCount>1</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>ZP_SoyBeen</harvestedThingDef>
			<harvestYield>15</harvestYield>
			<sowMinSkill>4</sowMinSkill>
			<immatureGraphicPath>Things/Plant/daizuplantimmature</immatureGraphicPath>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>8</growDays>
			<fertilitySensitivity>0.4</fertilitySensitivity>
			<visualSizeRange>
				<min>0.5</min>
				<max>1.0</max>
			</visualSizeRange>
			<sowResearchPrerequisites><li>ReBeans</li></sowResearchPrerequisites>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>ZP_PlantEdamame</defName>
		<label>枝豆</label>
		<description>畑のお肉である枝豆を育成できる。成長は早いが栄養価が低い。痩せた土地でも育つので重宝する。</description>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/edamameplant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
		<ingestible>
			<nutrition>0.18</nutrition>
		</ingestible>
		<plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<maxMeshCount>1</maxMeshCount>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>ZP_Edamame</harvestedThingDef>
			<harvestYield>15</harvestYield>
			<sowMinSkill>4</sowMinSkill>
			<immatureGraphicPath>Things/Plant/edamameplantimmature</immatureGraphicPath>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>3</growDays>
			<visualSizeRange>
				<min>0.5</min>
				<max>0.8</max>
			</visualSizeRange>
			<sowResearchPrerequisites><li>ReBeans</li></sowResearchPrerequisites>
		</plant>
	</ThingDef>
</Defs>
