<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==================== 飼い葉作成 ==================== -->
	<RecipeDef>
		<defName>MakeR_Manger10</defName>
		<label>飼い葉(10)</label>
		<description>藁と米ぬか、穀物をあわせて動物の飼料(高栄養素)を作る。</description>
		<jobString>飼い葉を作る</jobString>
		<workAmount>200</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workSkill>Cooking</workSkill>
		<skillRequirements>
			<Cooking>4</Cooking>
		</skillRequirements>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Straw</li>
					</thingDefs>
				</filter>
				<count>100</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RiceByproduct</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RawRice</li>
						<li>RawCorn</li>
						<li>RawPotatoes</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
		</ingredients>
		<products>
			<Manger>10</Manger>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Straw</li>
				<li>RawRice</li>
				<li>RawCorn</li>
				<li>RawPotatoes</li>
				<li>RiceByproduct</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkillLearnFactor>0.25</workSkillLearnFactor>
	</RecipeDef>
