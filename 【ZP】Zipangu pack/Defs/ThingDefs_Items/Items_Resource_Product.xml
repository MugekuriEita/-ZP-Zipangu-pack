	<!--=============== 飼料 ====================-->
	<ThingDef ParentName="OrganicProductBase">
		<defName>Manger</defName>
		<label>飼い葉</label>
		<description>家畜用飼料(高栄養素)</description>
		<stackLimit>300</stackLimit>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Hay</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(222,184,135)</color>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>10</MarketValue>
			<Mass>0.05</Mass>
			<DeteriorationRate>10</DeteriorationRate>
		</statBases>
		<ingestible>
			<foodType>Kibble</foodType>
			<preferability>RawBad</preferability>
			<tasteThought>AteKibble</tasteThought>
			<optimalityOffsetHumanlikes>-30</optimalityOffsetHumanlikes>
			<optimalityOffsetFeedingAnimals>15</optimalityOffsetFeedingAnimals>
			<nutrition>1.0</nutrition>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
		</ingestible>
		<thingCategories>
			<li>AnimalFeed</li>
		</thingCategories>
	</ThingDef>
